/*
 * Arm.h
 */


#ifndef ARM_H_
#define ARM_H_

class Cubelift : public Module {
private:

public:

	void ModuleInit(){

	}

	void OperatorControl(){
		bool liftButton = hw::stick->GetRawButton(1);
		bool lowerButton = hw::stick->GetRawButton(2);
		bool clawToggle = hw::stick->GetRawButton(3);

		if(liftButton){
			hw::armVic->Set(-.7);		//rotate motors to grab cube (left)
			hw::armVic->Set(-.7);		//rotate motors to grab cube (right)

		} else if(lowerButton){
			hw::armVic->Set(.7); //rotate motors opposite direction to eject cube (left)
			hw::armVic->Set(.7); //rotate motors opposite direction to eject cube (right)
		} else{
			hw::armVic->Set(0);		//No motion (left)
			hw::armVic->Set(0);		//No motion (right)
		}
	}
};

#endif /* ARM_H_ */



